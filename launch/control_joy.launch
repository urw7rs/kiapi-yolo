<launch>
    <param name="use_sim_time" value="false"/>

    <node name="joy_node" pkg="joy" type="joy_node" output="screen" respawn="false"/>
    <node name="twist_joy" pkg="yolo" type="control_joy.py" output="screen" respawn="false">
	<param name="x_speed_scale"  value="0.8" />
	<param name="w_speed_scale"  value="0.1" />
	<remap from="cmd_vel" to="cmd_vel"/>	
    </node>
</launch>
